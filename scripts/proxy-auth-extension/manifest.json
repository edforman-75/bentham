{
  "manifest_version": 3,
  "name": "Proxy Auth Helper",
  "version": "1.0",
  "description": "Handles proxy authentication for 2Captcha residential proxy",
  "permissions": [
    "proxy",
    "webRequest",
    "webRequestAuthProvider"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
